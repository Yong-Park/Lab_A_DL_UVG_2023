// afd
digraph {
	A [fillcolor="#7fff00" style=filled]
	B
	C [fillcolor="#ee3b3b" shape=doublecircle style=filled]
	D [fillcolor="#ee3b3b" shape=doublecircle style=filled]
	"" [shape=plaintext]
	"" -> A [label=""]
	A -> B [label=b]
	A -> C [label=a]
	B -> B [label=b]
	B -> C [label=a]
	C -> D [label=b]
}
