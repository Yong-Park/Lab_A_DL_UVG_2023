// afd
digraph {
	A [fillcolor="#7fff00" style=filled]
	B [fillcolor="#ee3b3b" shape=doublecircle style=filled]
	C [fillcolor="#ee3b3b" shape=doublecircle style=filled]
	D
	"" [shape=plaintext]
	"" -> A [label=""]
	A -> B [label=a]
	A -> C [label=b]
	B -> D [label=a]
	B -> D [label=b]
	C -> D [label=a]
	C -> D [label=b]
	D -> D [label=a]
	D -> D [label=b]
}
