// afd Minimizado
digraph {
	B
	E [fillcolor="#ee3b3b" shape=doublecircle style=filled]
	A [fillcolor="#7fff00" style=filled]
	D
	"" [shape=plaintext]
	E -> B [label=a]
	E -> A [label=b]
	D -> B [label=a]
	D -> E [label=b]
	B -> B [label=a]
	B -> D [label=b]
	"" -> A [label=""]
	A -> B [label=a]
	A -> A [label=b]
}
