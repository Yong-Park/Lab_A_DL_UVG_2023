// afd Minimizado
digraph {
	D
	F [fillcolor="#ee3b3b" shape=doublecircle style=filled]
	C
	A [fillcolor="#7fff00" style=filled]
	B
	"" [shape=plaintext]
	F -> D [label=a]
	F -> C [label=b]
	D -> C [label=a]
	D -> F [label=b]
	"" -> A [label=""]
	A -> B [label=a]
	A -> C [label=b]
	C -> C [label=a]
	C -> C [label=b]
	B -> D [label=a]
	B -> B [label=b]
}
