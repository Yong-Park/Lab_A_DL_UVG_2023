// afd Directo
digraph {
	A [fillcolor="#7fff00" style=filled]
	B
	C
	D
	E [fillcolor="#ee3b3b" shape=doublecircle style=filled]
	F
	G
	H [fillcolor="#ee3b3b" shape=doublecircle style=filled]
	"" [shape=plaintext]
	"" -> A [label=""]
	A -> B [label=a]
	A -> C [label=b]
	B -> D [label=a]
	B -> E [label=b]
	B -> F [label=c]
	C -> G [label=a]
	D -> E [label=b]
	E -> E [label=b]
	F -> H [label=b]
	F -> F [label=c]
	G -> H [label=a]
	H
}
